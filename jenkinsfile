       stage('One') {
         steps {
           echo 'Hi, this is Ravi'
               }
        }
	  stage('build') {
        steps {
          echo 'building'
              }
        }
      stage('test') {
        steps {
          echo 'testing'
              }
         }
      stage('integrating test') {
                  steps {
               echo 'testing'
                }
             }
    }
}

